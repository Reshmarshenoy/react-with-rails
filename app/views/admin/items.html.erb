
<link rel="import" href="<%= asset_path 'iron-form/iron-form.html' %>"/>    
<link rel="import" href="<%= asset_path 'paper-input/paper-input.html' %>"/>    
<link rel="import" href="<%= asset_path 'paper-checkbox/paper-checkbox.html' %>"/> 
<link rel="import" href="<%= asset_path 'paper-button/paper-button.html' %>"/>
<link rel="import" href="<%= asset_path 'iron-image/iron-image.html' %>"/>
<link rel="import" href="<%= asset_path 'google-map/google-map.html' %>"/>
<link rel="import" href="<%= asset_path 'google-youtube/google-youtube.html' %>"/>
<link rel="import" href="<%= asset_path 'elements/seperator-bar.html' %>"/> 
<link rel="import" href="<%= asset_path 'elements/header-title.html' %>"/> 

<div class="row">
  <div class="small-12 medium-12 large-12 columns">
    <heading-title>
      <h2>Form Elements</h2> 
      <div class="float-right">
        <paper-checkbox>Done</paper-checkbox>
      </div> 
    </heading-title>
    
    <form is="iron-form" id="form" method="post" action="#">
      <paper-input name="name" label="name"></paper-input>
      <paper-button raised onclick="submitForm(event)">Submit</paper-button>
    </form>
  </div>
</div>
<seperator-bar></seperator-bar>
<div class="row">
  <div class="small-12 large-12 columns">
    <iron-image 
      src="http://www.newschool.edu/uploadedImages/Parsons/Profiles/jamer_hunt_profile.jpg",
      error= true 
      fade = true 
      preload = true 
      loading = true 
    ></iron-image>
  </div>
</div>
<seperator-bar></seperator-bar>
<div class="row">
  <div class="small-12 medium-12 large-12 columns">
    <google-map latitude="37.77493" longitude="-122.41942" fit-to-markers>
      <google-map-marker latitude="37.779" longitude="-122.3892"
        draggable="true" title="Go Giants!"></google-map-marker>
      <google-map-marker latitude="37.777" longitude="-122.38911"></google-map-marker>
  </google-map>
  </div>
</div>
<seperator-bar></seperator-bar>
<div class="row">
  <div class="small-12 large-12 columns">
    <google-youtube
      video-id="uVdV-lxRPFo"
      height="360px"
      width="640px"
      fluid= true 
      rel="0"
      start="5"
      autoplay="0"
      id="youtube">
    </google-youtube>
  </div>
</div>

<script type="text/javascript">
    var form = document.getElementById('form')
    function submitForm (e){
      e.preventDefault()
      form.submit()
    }
  
    
    form.addEventListener('iron-form-presubmit', function(){
      console.log("Some Events")
    })
    var youtube  = document.getElementById('youtube')
    youtube.addEventListener('google-youtube-ready', function(){
      console.log("Youtube ready")
    })
    youtube.addEventListener('google-youtube-state-change', function(){
      console.log("Some changes")
    })
</script>